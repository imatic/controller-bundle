{
    "name": "imatic/controller-bundle",
    "type": "symfony-bundle",
    "description": "Imatic Controller Bundle",
    "keywords": [],
    "homepage": "http://imatic.cz",
    "license": "proprietary",
    "authors": [
        {
            "name": "Stepan Koci",
            "email": "stepan.koci@imatic.cz"
        },
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "http://imatic.cz/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.imatic.cz"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.6",
        "sensio/framework-extra-bundle": "3.0.*",
        "imatic/data-bundle": "~2.6",
        "genemu/form-bundle": "2.2.*"
    },
    "require-dev": {
        "imatic/testing-bundle": "~2.6"
    },
    "suggest": {
        "imatic/importexport-bundle": "Allows to use import and export apis"
    },
    "autoload": {
        "psr-4": {
            "Imatic\\Bundle\\ControllerBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.6.x-dev"
        }
    }
