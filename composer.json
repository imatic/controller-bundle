{
    "name": "imatic/controller-bundle",
    "type": "symfony-bundle",
    "description": "Imatic Controller Bundle",
    "keywords": [],
    "homepage": "http://imatic.cz",
    "license": "proprietary",
    "authors": [
        {
            "name": "Stepan Koci",
            "email": "stepan.koci@imatic.cz"
        },
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "http://imatic.cz/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://composer.imatic.cz"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.7|~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "imatic/data-bundle": "~3.0",
        "imatic/genemuform-bundle": "dev-master#c42e1beaec5a7ab270b7ff279a49553111a43f71",
    },
    "require-dev": {
        "imatic/testing-bundle": "~3.0",
        "phpunit/phpunit": "~5.2"
    },
    "suggest": {
        "imatic/importexport-bundle": "Allows to use import and export apis"
    },
    "autoload": {
        "psr-4": {
            "Imatic\\Bundle\\ControllerBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "secure-http": false
    }
}
