{
    "name": "imatic/controller-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to write simple controllers",
    "keywords": ["controller", "symfony"],
    "homepage": "https://www.imatic.cz",
    "license": "MIT",
    "authors": [
        {
            "name": "Stepan Koci",
            "email": "stepan.koci@imatic.cz"
        },
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "http://imatic.cz/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.imatic.cz"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "~3.1",
        "sensio/framework-extra-bundle": "~3.0",
        "imatic/data-bundle": "~3.0"
    },
    "require-dev": {
        "imatic/testing-bundle": "~3.0",
        "phpunit/phpunit": "~5.2",
        "imatic/view-bundle": "~3.0",
        "imatic/form-bundle": "~3.0"
    },
    "suggest": {
        "imatic/importexport-bundle": "Allows to use import and export apis"
    },
    "autoload": {
        "psr-4": {
            "Imatic\\Bundle\\ControllerBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
